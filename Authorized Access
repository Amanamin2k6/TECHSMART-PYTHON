"""
Dictionaries
Instruction Practice: Dictionary Iteration
Authorized Access
"""

#### ---- DATABASE ACCOUNTS ---- ####

## -- Account holders and their admin permissions -- ##

accounts = {
  "Kay Cattaneo": False,
  "Zaahira Matthiasen": True,
  "Theodore Ventura" : False,
  "Lina Alexander": True,
  "Paolo Eros": True,
  "Fatima Ajam": False,
  "Duha Lang": False,
  "Maria Olson": False,
  "Erik Gundersen": False,
  "Kim Falk": False,
  "Jacob Nilsen": False,
  "Carina Evansen": True
  }

## -- Admin and regular account totals -- ##
admin = list(accounts.values())

admin_count = admin.count(True)

reg_count = admin.count(False)

#### ---- OUTPUT ---- ####
print(str(admin_count) + " accounts have administrative access to the database.\n" + str(reg_count) + " accounts have limited access to the database.")

